#tag WindowCode
	#tag Event
		Sub Opening()
		  // attach three logwriters updating a listbox
		  var my_loging As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  my_loging.AddWriter ("LB1", New clListBoxLogWriter(ListBox1))
		  my_loging.AddWriter "LB2", New clListBoxLogWriter(ListBox4)
		  my_loging.AddWriter "LB3", New clListBoxLogWriter(ListBox5)
		  
		  my_loging.AddWriter "TF1", New clTextfileLogWriter( SpecialFolder.Desktop, "test%.txt","%")
		    
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events pbStartHA1
	#tag Event
		Sub Pressed()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.WriteInfo("Demo","test1")
		  
		  tmp.WriteWarning("Demo","test2")
		  
		  tmp.WriteInfo("Demo","Test message  %0 at %1", 12.35, "Alpha")
		  
		  tmp.StartTask "HA1"
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbEndHA1
	#tag Event
		Sub Pressed()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.WriteError("Demo", "test1")
		  
		  tmp.WriteWarning("Demo", "test2")
		  
		  
		  tmp.EndTask "HA1"
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbSummary
	#tag Event
		Sub Pressed()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.WriteSummary
		  tmp.MethodStatsAll
		  
		  tmp.EndTaskAll
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbStartHA2
	#tag Event
		Sub Pressed()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.WriteInfo("Demo","test1")
		  
		  tmp.WriteWarning("Demo","test2")
		  
		  tmp.StartTask "HA2"
		  
		  var d as DateTime = DateTime.Now
		  
		  tmp.WriteInfo("Demo","Another message at %0",d)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbEndHA2
	#tag Event
		Sub Pressed()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.WriteError("Demo", "test1")
		  
		  tmp.WriteWarning("Demo", "test2")
		  
		  tmp.EndTask "HA2"
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbEndAll
	#tag Event
		Sub Pressed()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.EndTaskAll
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Timer1
	#tag Event
		Sub Action()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.EnterMethod("Alpha")
		  timer3.Period = 100*Rnd
		  timer3.RunMode = timer.RunModes.Single
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Timer2
	#tag Event
		Sub Action()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.EnterMethod("Beta")
		  timer4.Period = 300*Rnd
		  timer4.RunMode = timer.RunModes.Single
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ckTimerTask1
	#tag Event
		Sub ValueChanged()
		  
		  
		  if me.Value then
		    timer1.RunMode = timer.RunModes.Multiple
		    
		  else
		    timer1.RunMode = timer.RunModes.Off
		    
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ckTimerTask2
	#tag Event
		Sub ValueChanged()
		  
		  
		  if me.Value then
		    timer2.RunMode = timer.RunModes.Multiple
		    
		  else
		    timer2.RunMode = timer.RunModes.Off
		    
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Timer3
	#tag Event
		Sub Action()
		  
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.ExitMethod("Alpha")
		  timer3.RunMode = timer.RunModes.Off
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Timer4
	#tag Event
		Sub Action()
		  var tmp As clLogManager = clLogManager.GetDefaultLogingSupport
		  
		  tmp.ExitMethod("Beta")
		  timer4.RunMode = timer.RunModes.Off
		  
		End Sub
	#tag EndEvent
#tag EndEvents
